{
  "presets": [
    ["@babel/preset-env", {
      "shippedProposals": true,
      "targets": {
        "electron": "1.8.3"
      }
    }]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    ["@babel/transform-react-jsx", { "pragma": "h" }]
  ],
  "ignore": [
    "node_modules"
  ]
}
