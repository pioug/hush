{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "no-unused-vars": [2, {
      "varsIgnorePattern": "^h$"
    }],
    "react/jsx-uses-vars": 2
  },
  "settings": {
    "react": {
      "pragma": "h"
    }
  }
}
